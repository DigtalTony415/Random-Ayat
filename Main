import javax.swing.*;


public class RandomAyat
{

public static void main (String[] args)
      {
       JFrame frame = new JFrame ("Ayat Selector");
       frame.setDefaultCloseOperation (JFrame.EXIT_ON_CLOSE);
       
        frame.getContentPane().add(new RAPanel());
        frame.pack();
        frame.setLocation(520, 32);
        frame.setVisible(true);

      }

      

}
